{
  "name":        "cme-os/cme",
  "description": "Campaigns Made Easy",
  "keywords":    [
    "framework",
    "laravel"
  ],
  "license":     "MIT",
  "require":     {
    "laravel/framework":            "4.2.*",
    "rmccue/requests":              "1.6.1",
    "way/generators":               "~2.0",
    "xethron/migrations-generator": "1.2.2",
    "aws/aws-sdk-php":              "2.7.18",
    "cme/cme-kernel":               "dev-master",
    "cme/cme-data":                 "@dev"
  },
  "autoload":    {
    "classmap": [
      "app/database/migrations",
      "app/database/seeds",
      "app/tests/TestCase.php"
    ],
    "psr-4":    {
      "Cme\\": "app/Cme"
    }
  },
  "scripts":     {
    "post-install-cmd":        [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd":         [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config":      {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
